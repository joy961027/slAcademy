package Score_Package;


public class Play {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student student = new Student();
		student.setScore(90, 80, 75);
		student.getTotScore();
		student.getAvgScore();
		student.putScore();
	}

}

