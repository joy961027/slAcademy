package Bank_Package;

import java.util.ArrayList;

public class Play {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayList<Bank> b1 = new ArrayList<>();
		for(int i=0; i<4; i++) {
			Bank b = new Bank();
			b1.add(b);
			b1.get(i).setBank();
		}
		for(int i=0; i<4; i++) {
			b1.get(i).putBank();
		}
		

	}

}
